<Application
    x:Class="Tethys.OBD.BluetoothTest.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone"
    xmlns:BluetoothTest="clr-namespace:Tethys.OBD.BluetoothTest">

    <!--Application Resources-->
    <Application.Resources>
        <BluetoothTest:LocalizedStrings x:Key="LocalizedStrings"/>
    </Application.Resources>

    <Application.ApplicationLifetimeObjects>
        <!--Required object that handles lifetime events for the application-->
        <shell:PhoneApplicationService
            Launching="ApplicationLaunching" Closing="ApplicationClosing"
            Activated="ApplicationActivated" Deactivated="ApplicationDeactivated"/>
    </Application.ApplicationLifetimeObjects>

</Application>